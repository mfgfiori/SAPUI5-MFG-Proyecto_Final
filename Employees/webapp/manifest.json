{
    "_version": "1.12.0",
    "sap.app": {
        "id": "projectfinal.Employees",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "dataSources": {
            "ZEMPLOYEES_SRV": {
                "uri": "sap/opu/odata/sap/ZEMPLOYEES_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "projectfinal.Employees.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "projectfinal.Employees.i18n.i18n"
                }
            },
            "odataModel": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Request",
                    "useBatch": false,
                    "metadataUrlParams": {
                    "sap-documentation": "heading"
                    }            
                },
                "dataSource": "ZEMPLOYEES_SRV",
                "preload": true                
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "projectfinal.Employees.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMainView",
                    "pattern": "",
                    "target": [
                        "TargetMainView"
                    ]
                },
                {
                    "name": "RouteCreateEmployee",
                    "pattern": "CreateEmployee",
                    "target": [
                        "TargetCreateEmployee"
                    ]
                },
                {
                    "name": "RouteShowEmployees",
                    "pattern": "ShowEmployees",
                    "target": [
                        "TargetShowEmployees"
                    ]
                }
            ],
            "targets": {
                "TargetMainView": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "MainView",
                    "viewName": "MainView"
                },
                "TargetCreateEmployee": {
                    "viewType": "XML",
                    "viewName": "CreateEmployee",
                    "transition": "slide"
                },
                "TargetShowEmployees": {
                    "viewType": "XML",
                    "viewName": "ShowEmployees",
                    "transition": "slide"
                }
            }
        }
    }
}